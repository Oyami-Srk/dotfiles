{
    "v": {
        "name": "+vim",
        "r": [":so ~/.config/nvim/init.vim\n:echo 'Reloaded!'", "reload-config"],
        "q": [":q", "quit-vim"],
        "Q": [":q!", "force-quit-vim"]
    },
    "w": {
        "name": "+window",
        "w": ["<C-W>w", "other-window"],
        "/": ["<C-W>v", "vertical-split"],
        "=": ["<C-W>s", "horizon-split"],
        "n": ["<C-W>n", "new-window"],
        "d": ["<C-W>c", "delete-window"],
        "D": ["<C-W>o", "delete-others"],
        "h": ["<C-W>h", "window-left"],
        "l": ["<C-W>l", "window-right"],
        "j": ["<C-W>j", "window-down"],
        "k": ["<C-W>k", "window-up"],
        "H": ["<C-W>H", "window-move-left"],
        "L": ["<C-W>L", "window-move-right"],
        "J": ["<C-W>J", "window-move-down"],
        "K": ["<C-W>K", "window-move-up"],
        "+": [":resize +5", "window-enlarge"],
        "-": [":resize -5", "window-shrink"],
        "[": ["<C-W>5<", "window-expand-left"],
        "]": ["<C-W>5>", "window-expand-right"],
        "b": ["<C-W>=", "window-size-blance"]
    },
    "g": {
        "name": "+goto",
        "d": ["<Plug>(coc-definition)", "goto-definition"],
        "t": ["<Plug>(coc-type-definition)", "goto-type-definition"],
        "i": ["<Plug>(coc-implementation)", "goto-implementation"],
        "r": ["<Plug>(coc-references)", "goto-references"],
        "[": ["<Plug>(coc-diagnostic-prev)", "goto-diagnostic-prev"],
        "]": ["<Plug>(coc-diagnostic-next)", "goto-diagnostic-next"],
        "f": ["<C-I>", "go-forward"],
        "b": ["<C-O>", "go-backward"]
    },
    ",": {
        "name": "+tools",
        "h": [":call CocAction('doHover')", "show-hover"],
        "s": [":CocList snippets", "snippets"],
        "p": {
            "name": "+package",
            "i": [":PlugInstall", "plug-install"]
        },
        "a": ["<Plug>(EasyAlign)", "easy-align"],
        "t": [":NERDTreeToggle", "nerdtree-toggle"]
    },
    "f": {
        "name": "+file",
        "f": [":Leaderf file", "open-file"],
        "w": [":w", "write-file"],
        "e": [":e ~/.config/nvim/init.vim", "open-config"],
        "t": [":NERDTreeToggle", "goto-type-definition"],
        "r": [":Ranger", "ranger"]
    },
    "<Tab>": [":bn", "next-buffer"],
    "b": {
        "name": "+buffer",
        "b": [":Leaderf buffer", "open-buffer"],
        "f": [":Leaderf buffer", "open-buffer"],
        "1": [":b1", "buffer-1"],
        "2": [":b2", "buffer-2"],
        "3": [":b3", "buffer-3"],
        "4": [":b4", "buffer-4"],
        "5": [":b5", "buffer-5"],
        "6": [":b6", "buffer-6"],
        "7": [":b7", "buffer-7"],
        "8": [":b8", "buffer-8"],
        "9": [":b9", "buffer-9"],
        "d": [":Bclose", "delete-buffer"],
        "n": [":bn", "next-buffer"],
        "p": [":bp", "prev-buffer"],
        "l": [":ls", "list-buffer"]
    },
    "1": [":1wincmd w", "window-1"],
    "2": [":2wincmd w", "window-2"],
    "3": [":3wincmd w", "window-3"],
    "4": [":4wincmd w", "window-4"],
    "5": [":5wincmd w", "window-5"],
    "6": [":6wincmd w", "window-6"],
    "7": [":7wincmd w", "window-7"],
    "8": [":8wincmd w", "window-8"],
    "9": [":9wincmd w", "window-9"],
    " ": "clear-highlight",
    ";": ["<Plug>NERDCommenterToggle", "toggle-comment"],
    "e": ["<Plug>(easymotion-prefix)", "easy-motion"],
    "c": {
        "name": "+comment",
        "t": ["<Plug>NERDCommenterToggle", "toggle-comment"],
        "c": ["<Plug>NERDCommenterNested", "comment-line"],
        "n": ["<Plug>NERDCommenterNested", "comment-nested"],
        "m": ["<Plug>NERDCommenterMinimal", "comment-minimal"],
        "i": ["<Plug>NERDCommenterInvert", "comment-invert"],
        "s": ["<Plug>NERDCommenterSexy", "comment-pretty"],
        "$": ["<Plug>NERDCommenterToEOL", "comment-toeol"],
        "A": ["<Plug>NERDCommenterAppend", "comment-append"],
        "a": ["<Plug>NERDCommenterAltDelims", "switch-delimiter"],
        "l": ["<Plug>NERDCommenterAlignLeft", "align-left"],
        "b": ["<Plug>NERDCommenterAlignBoth", "align-both"],
        "u": ["<Plug>NERDCommenterUncomment", "uncomment"]
    }
}


